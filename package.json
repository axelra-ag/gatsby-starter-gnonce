{
  "name": "lucky-gatsby-starter",
  "version": "1.0.1",
  "private": true,
  "description": "Re-implementation of the react template lucky-react-starter using Gatsby.js",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint --cache 'src/**/*.{ts,tsx,jsx}' && stylelint '**/**.tsx' && tsc && ts-unused-exports tsconfig.json",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "npm run lint"
  },
  "browserslist": {
    "production": [
      "edge >= 12",
      "ff >= 60",
      "chrome >= 44",
      "safari >= 10"
    ],
    "development": [
      "edge >= 12",
      "ff >= 60",
      "chrome >= 44",
      "safari >= 10"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "axelra-styled-bootstrap-grid": "^0.1.1",
    "lodash": "^4.17.20",
    "polished": "^3.6.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-scripts": "3.4.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0",
    "babel-plugin-styled-components": "^1.11.1",
    "gatsby": "^2.24.23",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-manifest": "^2.4.21",
    "gatsby-plugin-offline": "^3.2.21",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-routes": "^1.0.0",
    "gatsby-plugin-sharp": "^2.6.24",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-typescript": "^2.4.16",
    "gatsby-source-filesystem": "^2.3.23",
    "gatsby-transformer-sharp": "^2.5.12",
    "react-helmet": "^5.2.1",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/jest": "26.0.13",
    "@types/lodash": "^4.14.161",
    "@types/node": "14.10.0",
    "@types/react": "16.9.49",
    "@types/react-helmet": "^5.0.16",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-10x": "^1.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.1",
    "prettier": "^2.1.1",
    "redux-devtools-extension": "latest",
    "stylelint": "13.7.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-unused-exports": "^6.2.3",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.1.0",
    "typescript": "^4.0.2",
    "whatwg-fetch": "^3.4.1"
  }
}
